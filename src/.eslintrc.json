{
	"root": true,

	"extends": "../.eslintrc-browser.json",

	"rules": {
		// Strict mode is only enabled in the built file.
		"strict": "off"
	},

	"globals": {
		"Proxy": true,
		"Reflect": true,

		"jQuery": true,
		"migrateWarn": true,
		"migrateWarnFunc": true,
		"migrateWarnProp": true,
		"jQueryVersionSince": true,

		"noGlobal": true
	},

	"overrides": [
		{
			"files": "wrapper.js",
			"globals": {
				"jQuery": false
			}
		}
	]
}
